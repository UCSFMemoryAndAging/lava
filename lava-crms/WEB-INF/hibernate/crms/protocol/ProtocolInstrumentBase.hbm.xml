<?xml version="1.0"?> 

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"
    [<!ENTITY protocolInclude SYSTEM "classpath://../hibernate/crms/protocol/protocolInclude.xml" >
     <!ENTITY protocolFilterInclude SYSTEM "classpath://../hibernate/crms/protocol/protocolFilterInclude.xml" >]>
     
<hibernate-mapping>

<class name="edu.ucsf.lava.crms.protocol.model.ProtocolInstrumentBase" table="prot_node" select-before-update="true">

	&protocolInclude;
	
	<many-to-one name="visitBase" class="edu.ucsf.lava.crms.protocol.model.ProtocolVisitBase" column="parent_id" lazy="false"/>
	<many-to-one name="instrumentConfigBase" column="config_node_id" class="edu.ucsf.lava.crms.protocol.model.ProtocolInstrumentConfigBase" lazy="false"/>	
	
	&protocolFilterInclude;
	
</class>	
	
		
</hibernate-mapping>
