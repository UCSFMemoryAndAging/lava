<?xml version="1.0"?> 

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
     
<hibernate-mapping>

<joined-subclass name="edu.ucsf.lava.crms.protocol.model.PatientProtocolInstrument" table="ptprot_instr" extends="edu.ucsf.lava.crms.protocol.model.PatientProtocolInstrumentBase" select-before-update="true">
	<key column="node_id"/>
	<many-to-one name="instrument" column="InstrID" class="edu.ucsf.lava.crms.assessment.model.Instrument"/>	
	<property name="collectWinStatus" column="collect_win_status" type="string" length="25"/>
 	<property name="collectWinReason" column="collect_win_reason" type="string" length="25"/>
 	<property name="collectWinNote" column="collect_win_note" type="string" length="100"/>
	<property name="customCollectWinStart" column="cust_collect_win_start" type="date"/>
	<property name="customCollectWinEnd" column="cust_collect_win_end" type="date"/>
</joined-subclass>
	
</hibernate-mapping>
