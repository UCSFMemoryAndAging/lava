<?xml version="1.0"?>
    
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"
    [<!ENTITY protocolOptionConfigInclude SYSTEM "classpath://../hibernate/crms/protocol/protocolOptionConfigInclude.xml" >
     <!ENTITY protocolConfigFilterInclude SYSTEM "classpath://../hibernate/crms/protocol/protocolConfigFilterInclude.xml" >]>
    
<hibernate-mapping>

	<class name="edu.ucsf.lava.crms.protocol.model.ProtocolVisitOptionConfigBase" table="prot_opt_config" select-before-update="true">

		&protocolOptionConfigInclude;
		
		<many-to-one name="visitBase" class="edu.ucsf.lava.crms.protocol.model.ProtocolVisitConfigBase" column="parent_id" not-null="true" cascade="none"/>

		&protocolConfigFilterInclude;
		
	</class>		

</hibernate-mapping>



