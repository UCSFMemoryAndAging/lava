<?xml version="1.0"?>
    
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<joined-subclass name="edu.ucsf.lava.crms.logiccheck.model.InstrumentLogicCheck" table="instrumentlogiccheck" extends="edu.ucsf.lava.core.logiccheck.model.LogicCheck" select-before-update="true">
		
		<key column="CheckDefId"/>
		
		<property name="instrVers" column="instrVers" type="string"/>
		<property name="visitTypes" column="visitTypes" type="string"/>
		<property name="checkCodes" column="checkCodes" type="string"/>
	    
	</joined-subclass>
	
</hibernate-mapping>


